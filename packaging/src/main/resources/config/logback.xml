<configuration>

	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>[%d{HH:mm:ss.SSS}] %-5level - %msg%n</pattern>
		</encoder>
	</appender>
	
	<logger name="org.springframework.migrationanalyzer" level="info"/>

	<root level="warn">
		<appender-ref ref="STDOUT" />
	</root>

</configuration>
